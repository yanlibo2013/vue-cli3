    "loadsh": "0.0.4",
    "nodemon": "^2.0.2",
    "vue": "^2.6.10",
    "vue-route": "^1.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-dll": "^1.1.11",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
