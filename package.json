{
  "name": "vue-cli3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dll": "vue-cli-service dll",
    "serve:vpn": "vue-cli-service serve --host 192.168.31.74 --open",
