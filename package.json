{
  "name": "vue-cli3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run serve",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dll": "vue-cli-service dll",
    "nodemon-server-routes": "nodemon --watch mock --exec json-server --routes ./server/db/routes.json ./server/db/db.json --port 7000",
    "nodemon-server-routes:vpn": "nodemon --watch mock --exec json-server --routes ./server/db/routes.json ./server/db/db.json --port 7000 --host 192.168.31.74",
    "deploy:dev": "npm run build && cross-env NODE_ENV=development node ./deploy",
    "deploy:prod": "npm run build && cross-env NODE_ENV=production node ./deploy"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "axios": "^0.19.2",
    "core-js": "^3.4.4",
    "element-ui": "^2.13.0",
    "fuse.js": "^3.4.6",
    "js-cookie": "^2.2.1",
    "jsplumb": "^2.12.8",
    "loadsh": "0.0.4",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "normalize.css": "^8.0.1",
    "panzoom": "^9.1.0",
    "vue": "^2.6.10",
    "vue-count-to": "^1.0.13",
    "vue-drag-drop": "^1.1.4",
    "vue-i18n": "^8.15.3",
    "vue-meta": "^2.3.2",
    "vue-route": "^1.5.1",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-dll": "^1.1.11",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
